pipeline {
    agent { label 'master' }
    stages {
        stage('build') {
            steps {
                echo "Hello test branch!"
                script {
                    COMMIT_HASH = sh (
                    script: '''#!/bin/bash
                    git show | grep -i -m 1 commit | awk '{print $2}' 
                    ''',
                    returnStdout: true).trim()
                }
                echo "Commit hash: ${COMMIT_HASH}"
                 
            }
        }
    }
}
